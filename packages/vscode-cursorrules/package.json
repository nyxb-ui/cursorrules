{
   "name": "cursorrules",
   "displayName": "Cursorrules",
   "description": "An extension for Cursor that lets you pull .cursorrules files from cursorrul.es",
   "version": "0.2.2",
   "author": "Nyxb",
   "publisher": "Nyxb",
   "icon": "images/icon.png",
   "engines": {
      "vscode": "^1.96.0"
   },
   "categories": ["AI", "Other"],
   "main": "./out/extension.js",
   "contributes": {
      "commands": [
         {
            "command": "cursorRules.addRule",
            "title": "Cursor Rules: Add .cursorrules"
         }
      ]
   },
   "scripts": {
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "vscode:prepublish": "pnpm run compile",
      "pack": "vsce package --no-dependencies",
      "publish": "vsce publish --no-dependencies"
   },
   "devDependencies": {
      "@biomejs/biome": "^1.9.4",
      "@types/node": "^22.13.1",
      "@types/vscode": "^1.96.0",
      "typescript": "^5.6.3"
   },
   "dependencies": {
      "axios": "^1.7.9"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/nyxb-ui/cursorrules/packages/vscode-cursorrules"
   }
}
